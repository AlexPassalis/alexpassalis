{
  "name": "fastify",
  "version": "1.0.0",
  "main": "./src/main.ts",
  "scripts": {
    "postgres:generate": "cross-env NODE_ENV=development npx drizzle-kit generate --config ./drizzle.config.ts",
    "postgres:migrate": "cross-env POSTGRES_MIGRATING=true npx drizzle-kit push --config ./drizzle.config.ts",
    "dev": "tsx watch src/main.ts",
    "lint": "eslint src",
    "build": "tsc",
    "start": "node build/src/main.js"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/oauth2": "^8.1.1",
    "@react-email/components": "^0.0.32",
    "cross-env": "^7.0.3",
    "dotenv-expand": "^12.0.1",
    "dotenv-flow": "^4.1.0",
    "drizzle-orm": "^0.39.0",
    "fastify": "^5.2.1",
    "fastify-metrics": "^12.1.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.12.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.8",
    "drizzle-kit": "^0.30.3",
    "eslint": "^9.19.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  }
}
